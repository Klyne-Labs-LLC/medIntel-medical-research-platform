# AI Configuration
GOOGLE_AI_API_KEY=<Your_Gemini_API_Key>
GROQ_API_KEY=<Your_Groq_API_Key_Fallback>

# Medical MCP Paths
ALGOLIA_MCP_NODE_PATH=<Path_to_Algolia_MCP_Node>/src/app.ts
PUBMED_MCP_NODE_PATH=<Path_to_PubMed_MCP>/src/app.ts
CLINICAL_TRIALS_MCP_PATH=<Path_to_Clinical_Trials_MCP>/src/app.ts
MEDICAL_DB_MCP_PATH=<Path_to_Medical_DB_MCP>/src/app.ts
IMAGING_MCP_PATH=<Path_to_Medical_Imaging_MCP>/src/app.ts

# Medical Database Configuration
ALGOLIA_APP_ID=<Algolia_App_ID>
ALGOLIA_API_KEY=<Algolia_API_Key>
ALGOLIA_MEDICAL_INDEX=medical_literature
PUBMED_API_KEY=<PubMed_API_Key>
CLINICAL_TRIALS_API_KEY=<Clinical_Trials_API_Key>
NCBI_API_KEY=<NCBI_API_Key>

# Security & HIPAA Compliance
ENCRYPTION_KEY=<256_bit_encryption_key>
JWT_SECRET=<JWT_Secret_Key>
SESSION_SECRET=<Session_Secret>
AUDIT_LOG_LEVEL=info
HIPAA_AUDIT_ENABLED=true

# Medical AI Configuration
AI_MODEL_PREFERENCE=gemini
AI_CONFIDENCE_THRESHOLD=0.7
ENABLE_MEDICAL_SAFETY_FILTERS=true
REQUIRE_MEDICAL_DISCLAIMER=true
MAX_IMAGE_SIZE_MB=50
SUPPORTED_IMAGE_FORMATS=jpg,jpeg,png,dicom,tiff

# Server Configuration
PORT=3000
HOST=127.0.0.1
NODE_ENV=development
CORS_ORIGINS=http://127.0.0.1:5000,http://localhost:5000

# Rate Limiting
API_RATE_LIMIT_WINDOW_MS=900000
API_RATE_LIMIT_MAX_REQUESTS=100
MEDICAL_API_RATE_LIMIT_MAX=50

# Medical Tools Configuration
MCP_ENABLED_TOOLS=search,analytics,pubmed_search,clinical_trials,image_analysis,differential_diagnosis
MEDICAL_SPECIALTIES=cardiology,neurology,oncology,radiology,dermatology,pathology,internal_medicine
EVIDENCE_LEVELS=systematic_review,meta_analysis,randomized_controlled_trial,cohort_study,case_series